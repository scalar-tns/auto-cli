{
  "title": "Title of the application",
  "description": "description of the application",
  "outfolder": "path",
  "cli": "dotnet new console -f net6.0 -n {NAME} -o {PATH}",
  "repo": "repo-name",
  "packages": [
    {
      "name": "System.CommandLine",
      "version": "--prerelease"
    },
    {
      "name": "System.CommandLine.Hosting",
      "version": "--prerelease"
    },
    {
      "name": "System.CommandLine.NamingConventionBinder",
      "version": "--prerelease"
    }
  ],
  "commands": [
    {
      "name": "subcommand",
      "command": "upcommand",
      "symbol": "cmdlet",
      "description": "description",
      "setverbosity": "bool"
    }
  ],
  "arguments": [
    {
      "name": "argument",
      "type": "string",
      "command": "cmd",
      "symbol": "<arg>",
      "defaultvalue": "value",
      "description": "description"
    }
  ],
  "options": [
    {
      "name": "option",
      "type": "string",
      "command": "cmd",
      "required": "true",
      "symbol": "--option",
      "alias": "-o",
      "defaultvalue": "value",
      "description": "description"
    }
  ]
}