{
  "outfolder": "path",
  "cli": "cd {PATH} ; dotnet new console -f net6.0 -n {NAME} -o .",
  "repo": "repo-name",
  "packages": [
    {
      "name": "System.CommandLine",
      "cli": "dotnet add package System.CommandLine --prerelease",
      "replace": "PACKAGE"
    },
    {
      "name": "System.CommandLine.Hosting",
      "cli": "dotnet add package System.CommandLine.Hosting --prerelease",
      "replace": "PACKAGE"
    }
  ],
  "commands": [
    {
      "name": "cmd",
      "type": "string",
      "call": "cmdlet",
      "decription": {
        "object": "description",
        "replace": "cmd.DESCSRIPTION"
      }
    }
  ],
  "subcommands": [
    {
      "name": "subcmd",
      "parent": "cmd",
      "type": "string",
      "call": "subcmdlet",
      "alias": {
        "present": "true",
        "value": "-c"
      },
      "default": {
        "present": "true",
        "value": "value"
      },
      "decription": {
        "object": "description",
        "replace": "subcmd.DESCSRIPTION"
      }
    }
  ],
  "arguments": [
    {
      "name": "arg",
      "parent": "cmd",
      "type": "string",
      "cmdlet": "arg",
      "default": {
        "present": "true",
        "value": "value"
      },
      "decription": {
        "object": "description",
        "replace": "arg.DESCSRIPTION"
      }
    }
  ],
  "options": [
    {
      "name": "option",
      "parent": "cmd",
      "type": "string",
      "required": "true",
      "cmdlet": "--option",
      "alias": {
        "present": "true",
        "value": "-o"
      },
      "default": {
        "present": "true",
        "value": "value"
      },
      "decription": {
        "object": "description",
        "replace": "option.DESCSRIPTION"
      }
    }
  ]
}